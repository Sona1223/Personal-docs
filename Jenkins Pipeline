pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Hello World'
            }
        }
    
        stage('Build') {
            steps {
                echo 'Welcome to hcl'
            }
        }
   
        stage('Deployment') {
            steps {
                echo 'Expert in DevOps'
            }
        }
         stage('test') {
            steps {
                echo 'demo in DevOps'
            }
        }
         stage('Delivery') {
            steps {
                echo 'training in DevOps'
            }
        }
    }
}
