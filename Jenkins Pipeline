pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Hello World'
            }
        }
    
        stage('Build') {
            steps {
                echo 'Welcome to DevOps'
            }
        }
   
        stage('Deploy') {
            steps {
                echo 'Expert in DevOps'
            }
        }
    }
}
